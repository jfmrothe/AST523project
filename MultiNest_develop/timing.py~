#!/usr/bin/env python 

import os
import sys
import subprocess
import numpy as np
import time


Ns = [3,5,10,50,100,1000]
Nrep = 100

for N in Ns:
    logZ = []
    logZerr = []
    for i in range(Nrep):
        time.sleep(1)
        os.system('./run '+str(N)+' > test.txt')
        fileHandle = open ( 'test.txt',"r" )
        lineList = fileHandle.readlines()
        fileHandle.close()
        logZ.append(lineList[-1].split(" ")[-3])
        logZerr.append(lineList[-1].split(" ")[-1][0:-1]) #removes final \n
    print N
    for entry in logZ:
        print entry," ",
    print
    for entry in logZerr:
        print entry," ",
    print
